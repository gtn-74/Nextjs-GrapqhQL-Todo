generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Todo {
  id String @id @default(cuid()) // 主キー: @id、 初期値:@default  UUID的なランダムで生成されたid:cuid
  content String
  done Boolean @default(false)
  createAt String
}

